{
  "presets": [
    ["@babel/preset-env", {
      "exclude": [
        "transform-regenerator"
      ]
    }],
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    ["module:fast-async", {
      "compiler": {
        "generators": false,
        "runtimePatter": ".js"
      }
    }],
    "module:babel-plugin-macros",
  ]
}
